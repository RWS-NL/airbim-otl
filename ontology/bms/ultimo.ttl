@prefix ultimo: <https://www.rws.nl/bms/ultimo/def#> .
@prefix nta: <https://w3id.org/def/basicsemantics-owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@base <https://www.rws.nl/bms/ultimo/def#> .

<https://www.rws.nl/bms/ultimo/def> rdf:type owl:Ontology ;
                                      owl:imports <https://w3id.org/def/basicsemantics-owl> ;
                                      rdfs:comment "Dit model is gebaseerd op Ultimo RWS in juni 2020. Het is gemodelleerd met als uitgangspunt het OWL topmodel van de NTA 8035. De scope is beperkt tot bruggen."@nl ;
                                      rdfs:label "Ultimo model t.b.v. Minicase"@nl .

#################################################################
#    Annotation properties
#################################################################

###  https://www.rws.nl/bms/ultimo/def#naam
ultimo:naam rdf:type owl:AnnotationProperty ;
      rdfs:subPropertyOf rdfs:label .


#################################################################
#    Object Properties
#################################################################

###  https://www.rws.nl/bms/ultimo/def#district
ultimo:district rdf:type owl:ObjectProperty ;
          rdfs:domain nta:PhysicalObject ;
          rdfs:range ultimo:District .


###  https://www.rws.nl/bms/ultimo/def#onderhoudstrategie
ultimo:onderhoudstrategie rdf:type owl:ObjectProperty ;
                    rdfs:range ultimo:Onderhoudstrategie .


###  https://www.rws.nl/bms/ultimo/def#organisatieonderdeel
ultimo:organisatieonderdeel rdf:type owl:ObjectProperty ;
                      rdfs:domain nta:PhysicalObject ;
                      rdfs:range ultimo:Organisatieonderdeel .


#################################################################
#    Data properties
#################################################################

###  https://www.rws.nl/bms/ultimo/def#bouwjaar
ultimo:bouwjaar rdf:type owl:DatatypeProperty ;
          skos:prefLabel "Bouwjaar"@nl .


###  https://www.rws.nl/bms/ultimo/def#topcode
ultimo:topcode rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:range xsd:string ;
         skos:prefLabel "Topcode"@nl .


#################################################################
#    Classes
#################################################################

###  https://www.rws.nl/bms/ultimo/def#Complex
ultimo:Complex rdf:type owl:Class ;
         rdfs:subClassOf nta:PhysicalObject ;
         ultimo:naam "Complex"@nl .


###  https://www.rws.nl/bms/ultimo/def#District
ultimo:District rdf:type owl:Class ;
          rdfs:subClassOf nta:EnumerationType .


###  https://www.rws.nl/bms/ultimo/def#Elementen
ultimo:Elementen rdf:type owl:Class ;
           rdfs:subClassOf nta:PhysicalObject ;
           skos:prefLabel "Elementen"@nl .


###  https://www.rws.nl/bms/ultimo/def#Objecten
ultimo:Objecten rdf:type owl:Class ;
          rdfs:subClassOf nta:PhysicalObject ;
          skos:prefLabel "Objecten"@nl .


###  https://www.rws.nl/bms/ultimo/def#Onderhoudstrategie
ultimo:Onderhoudstrategie rdf:type owl:Class ;
                    rdfs:subClassOf nta:EnumerationType .


###  https://www.rws.nl/bms/ultimo/def#Organisatieonderdeel
ultimo:Organisatieonderdeel rdf:type owl:Class ;
                      rdfs:subClassOf nta:EnumerationType .


